{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "allowJs": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "types": [
      "node",
      "jest"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "@app/shared/styles": [
        "libs/shared/styles/src/index.ts"
      ],
      "@app/auth": [
        "libs/auth/src/index.ts"
      ],
      "@/api/*": [
        "api/*"
      ],
      "@/components/*": [
        "components/*"
      ],
      "@/constants/*": [
        "constants/*"
      ],
      "@/contexts/*": [
        "contexts/*"
      ],
      "@/hooks/*": [
        "hooks/*"
      ],
      "@/redux/*": [
        "redux/*"
      ],
      "@/routing/*": [
        "routing/*"
      ],
      "@/services/*": [
        "services/*"
      ],
      "@/stores/*": [
        "stores/*"
      ],
      "@/types/*": [
        "types/*"
      ],
      "@/utils/*": [
        "utils/*"
      ]
    },
    "target": "es2015",
    "lib": [
      "es2017",
      "dom"
    ],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "module": "esnext",
    "moduleResolution": "node",
    "incremental": true
  },
  "include": [],
  "files": [],
  "references": [
    {
      "path": "./tsconfig.spec.json"
    }
  ],
  "exclude": [
    "node_modules"
  ]
}